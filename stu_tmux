tmux kill-session -t connections
tmux new-session -s connections -n server -d
tmux send-keys -t connections 'cd ~/code/connections' C-m
tmux send-keys -t connections 'tilt up' C-m
tmux new-window -n vim -t connections
tmux send-keys -t connections:2 'cd ~/code/connections' C-m
tmux send-keys -t connections:2 'vim -N' C-m
tmux attach -t connections
